# Environment variables for the graph knowledge project
# Copy this file to .env and fill in your actual values

# OpenAI API Key - Get this from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Authentication secret - Generate a random string for production
AUTH_SECRET=your_auth_secret_here

# Optional: Set to development for local development
NODE_ENV=development
